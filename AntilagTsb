while wait() do
-- Antilag V3 TSB
local folderName = "Thrown"
local workspace = game:GetService("Workspace")

-- Function to delete all Parts in the specified folder
local function deleteAllPartsInFolder(folder)
    for _, item in ipairs(folder:GetChildren()) do
        if item:IsA("Part") then
            item:Destroy()
        end
    end
end

-- Function to prevent new Parts from being created in the folder
local function onPartAdded(part)
    if part.Parent == workspace:FindFirstChild(folderName) then
        part:Destroy()
    end
end

-- Get the folder
local folder = workspace:FindFirstChild(folderName)
if folder then
    -- Delete existing Parts in the folder
    deleteAllPartsInFolder(folder)
end

-- Connect to the ChildAdded event to prevent new Parts from being created
workspace.ChildAdded:Connect(function(child)
    if child:IsA("Part") and child.Parent == workspace:FindFirstChild(folderName) then
        onPartAdded(child)
    end
end)
end
